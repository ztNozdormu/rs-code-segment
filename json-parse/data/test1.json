{
    "animation": {
      "default": "400ms ease-in",
      "fast": "300ms ease-in"
    },
    "breakpoints": {
      "mobile": "320px",
      "tablet": "768px",
      "computer": "992px",
      "desktop": "1200px",
      "widescreen": "1920px",
      "high": 2000,
      "wide": 1500,
      "long": 1600

    },
    "colors": {
      "text": "#111212",
      "background": "#fff",
      "primary": "#005CDD",
      "secondary": "#6D59F0",
      "muted": "#f6f6f9",
      "gray": "#D3D7DA",
      "highlight": "hsla(205, 100%, 40%, 0.125)",
      "white": "#FFF",
      "black": "#111212"
